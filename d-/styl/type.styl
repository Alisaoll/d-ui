@import 'variable.styl'
a{
    color: $txt-link;  
}
h1{
    font-size:$font-size-18;
}
h2{
    font-size: $font-size-19;
}
h3,h4{
    font-size: $font-size-16;
}
h5,.{$pf}txt-sub{
    font-size: $font-size-14;
}
h6,.{$pf}txt-tips{
    font-size: $font-size-12;
}
.{$pf}font-12{
     font-size: $font-size-12;
}
.{$pf}txt-default{
    color: $txt-default;
}
.{$pf}txt-white{
    color: white;
}
.{$pf}txt-info{
    color: $txt-info;
}
.{$pf}txt-muted{
    color: $txt-muted;
}
.{$pf}txt-warning,.{$pf}txt-red{
    color: $txt-warning;
}
.{$pf}txt-primary{
    color:#00a5e0;
}
.{$pf}txt-10{
    font-size:$font-size-10 !important;
}