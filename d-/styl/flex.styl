@import 'variable.styl'
.{$pf}flex-box{
  width: 100%;
  -webkit-box-sizing: border-box;
  min-height: 2.2rem;
  display: -webkit-box;
  display: -webkit-flex; 
  display: flex; 
}
.{$pf}flex{
  display: -webkit-box;
  display: -webkit-flex; 
  display: flex; 
}

.{$pf}justify-content{
  -webkit-justify-content:space-between;
  justify-content:space-between;
  }
  .{$pf}justify-content-c{
  -webkit-justify-content:center;
  justify-content:center;
  }
  .{$pf}justify-content-end{
  -webkit-justify-content:flex-end;
  justify-content:flex-end;
  }
  .{$pf}flex-box-item{
    -webkit-box-flex: 1;
     -moz-box-flex:1;
    box-flex:1;
    width: 100%;
    text-align: center;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: center;
    -webkit-box-align: center; 
}
.{$pf}flex-item-center{
    -webkit-box-align: center;
    -moz-box-align:center;
    box-align:center;
    -webkit-align-items:center;
    align-items: center;
    -webkit-justify-content:space-between;
   justify-content:space-between;
}
.{$pf}flex-pack-start{
      -webkit-box-pack: start;
}
.{$pf}flex-pack-end{
      -webkit-box-pack: end;
}
.{$pf}flex-pack-center{
      -webkit-box-pack: center;
}
.{$pf}flex-align-start {
  -webkit-box-align: start; }

.{$pf}flex-align-end {
  -webkit-box-align: end; }

.{$pf}flex-align-center {
  -webkit-box-align: center; }


.{$pf}flex-row {
  display: block;
  overflow: hidden; }

.{$pf}flex-col {
  float: left;
  box-sizing: border-box;
  width: 100%;}
.{$pf}flex-col-10 {
  width: 10%; }

.{$pf}flex-col-20 {
  width: 20%; }

.{$pf}flex-col-25 {
  width: 25%; }

.{$pf}flex-col-33 {
  width: 33.3333%; }

.{$pf}flex-col-50 {
  width: 50%; }

.{$pf}flex-col-67 {
  width: 66.6666%; }

.{$pf}flex-col-75 {
  width: 75%; }

.{$pf}flex-col-80 {
  width: 80%; }

.{$pf}flex-col-90 {
  width: 90%; }
  .{$pf}flex-col-100 {
  width: 100%; }



